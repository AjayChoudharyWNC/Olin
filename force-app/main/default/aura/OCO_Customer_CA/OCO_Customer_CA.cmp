<aura:component controller="OCO_CustomerController">
    <aura:attribute name="sapId" type="String"/>
    <aura:attribute name="sapRecord" type="SAP_Onboarding__c"/>
    <aura:attribute name="taxFileList" type="List" default="[]"/>
    <aura:attribute name="bankReferenceFileList" type="List" default="[]"/>
    <aura:attribute name="tradeReferenceFileList" type="List" default="[]"/>
    <aura:attribute name="financialFileList" type="List" default="[]"/>
    <aura:attribute name="vatList" type="List"/>
    <aura:attribute name="languageLabelMap" type="Object"/>
    <aura:attribute name="englishLanguageLabelMap" type="Object"/>
    <aura:attribute name="showTradeRefFields" type="boolean" default="true"/>
    <aura:attribute name="showBankRefFields" type="boolean" default="true"/>
    <aura:attribute name="spinner" type="boolean"/>
    <aura:attribute name="showCreditInfo" type="boolean" default="true"/>
    <aura:attribute name="appUserId" type="String"/>
    <aura:attribute name="screenName" type="String"/>
    <aura:attribute name="sapNo" type="String" default=""/>
    <aura:attribute name="showConfirmDialog" type="boolean"/>
    <aura:attribute name="confirmMessage" type="String" default=""/>
    <aura:attribute name="confirmCancelLabel" type="String" default=""/>
    <aura:attribute name="confirmSubmitLabel" type="String" default=""/>
    <aura:attribute name="toDeleteFileId" type="String"/>
    <aura:attribute name="fileType" type="String" default=""/>
    <aura:attribute name="confirmEventName" type="String" default=""/>
    <aura:attribute name="parent" type="aura.component" />
    <aura:attribute name="isFormSubmitManual" type="boolean"/>
    <aura:registerEvent name="refreshSapRecordEvent" type="c:OCO_Customer_RefreshSapRecord"/>
    <aura:attribute name="isDisabledForProfile" type="boolean"/>
    <aura:attribute name="taxTypeDependentMap" type="Object"/>
    <aura:attribute name="taxTypeDependentIdMap" type="Map"/>
    <aura:attribute name="uploadFileText" type="String" default="Upload File"/>
    <aura:attribute name="languageFileData" type="Object"/>
    <aura:attribute name="isTaxTypeLoaded" type="boolean"/>
    <aura:attribute name="creditType" type="String" default="Full"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:method name="callDoInit" action="{!c.doInit}"/>
    <!--aura:handler event="c:OCO_HandleLanguageChange_CA" action="{!c.handleTaxOptions}"/-->
    
    <aura:html tag="style">
        :root {
        --fileContent: {!v.uploadFileText};
        }
    </aura:html>
    
    <aura:if isTrue="{!v.spinner}">
        <div aura:id="spinnerId" style="position:fixed;" class="slds-spinner_container">
            <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                <span class="slds-assistive-text">{!v.languageLabelMap.PS_45}...</span>
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
    </aura:if>
    <aura:if isTrue="{!v.showConfirmDialog}">
        <div role="dialog" class="slds-modal slds-fade-in-open ">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <h1 class="slds-text-heading--medium">{!v.languageLabelMap.HP_30}</h1>
                </header>
                <div class="slds-modal__content slds-p-around--medium">
                    <center><b>{!v.confirmMessage}</b></center>
                </div>
                <footer class="slds-modal__footer">
                    <lightning:button name='No' label='{!v.confirmCancelLabel}' onclick='{!c.handleConfirmDialog}'/>
                    <aura:if isTrue="{!v.confirmEventName != 'fileSize'}"><lightning:button variant="brand" name='{!v.confirmEventName}' label='{!v.confirmSubmitLabel}' onclick='{!c.handleConfirmDialog}'/></aura:if>
                </footer>
            </div>
        </div>
        <div class="slds-backdrop slds-backdrop--open"></div>            
    </aura:if>
    
    <lightning:recordEditForm objectApiName="SAP_Onboarding__c" recordId="{!v.sapId}" aura:id="CAForm" onsuccess="{!c.handleFormSuccess}" onsubmit="{!c.handleFormSubmit}" onerror="{!c.handleError}">
        <div class="slds-grid slds-wrap" style="min-width:100%;">
                <div class="slds-col slds-large-size_3-of-8 slds-medium-size_8-of-8 slds-small-size_8-of-8 slds-m-bottom_medium">
                    <b><span style="color:#767676;font-size:18px;">{!v.languageLabelMap.CI_1} : {!v.sapNo}</span><br/><span style="color:#a1a1a1;font-size:16px;">{!v.screenName == 'Customer Information' ? v.languageLabelMap.CI_2 : v.screenName == 'Document Delivery' ? v.languageLabelMap.DD_7 : v.screenName == 'Product Stewardship' ? v.languageLabelMap.CA_83 : v.screenName == 'Credit Application' ? v.languageLabelMap.HP_6 : v.screenName}</span></b>
                </div>
                <div class="slds-col slds-large-size_5-of-8 slds-medium-size_8-of-8 slds-small-size_8-of-8 slds-grid slds-wrap slds-text-align_right slds-m-top_small">
                    <div class="slds-col slds-large-size_1-of-1 slds-medium-size_1-of-1 slds-small-size_1-of-1">
                        <button class="slds-button slds-button_neutral btn" onclick="{!c.handleSaveProgress}"  type="button" disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'),v.isDisabledForProfile)}">
                            <lightning:icon iconName="utility:save" size="x-small" class="shareIcon" style="margin-right:5px;"/>{!v.languageLabelMap.CI_3}
                        </button>
                        <button class="slds-button slds-button_neutral btn" type="button" onclick="{!c.handleShareWithColleague}" disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'),v.isDisabledForProfile)}">
                            <lightning:icon iconName="utility:socialshare" size="x-small" class="shareIcon" style="margin-right:5px;"/>{!v.languageLabelMap.CI_4}
                        </button>
                        <aura:if isTrue="{!v.creditType == 'Full'}">
                            <button class="slds-button slds-button_neutral btn" type="submit" disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'),v.isDisabledForProfile)}">
                                <lightning:icon iconName="utility:success" size="x-small" class="shareIcon" style="margin-right:5px;"/>{!v.languageLabelMap.CI_5}
                            </button>
                            <aura:set attribute="else">
                                <button class="slds-button slds-button_neutral btn" onclick="{!c.handleSubmitPartialCredit}" disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'),v.isDisabledForProfile)}">
                                    <lightning:icon iconName="utility:success" size="x-small" class="shareIcon" style="margin-right:5px;"/>{!v.languageLabelMap.CI_5}
                                </button>
                            </aura:set>
                        </aura:if>
                    </div>
                </div>
            <aura:if isTrue="{!v.creditType == 'Full'}">
                <div class="slds-col slds-size_1-of-1" style="border-bottom:2px solid #767676;color:#767676;font-size: 14px;font-weight:bold;">{!v.languageLabelMap.CA_1}</div>
                <lightning:messages />
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr>{!v.languageLabelMap.CA_2}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}"  fieldName="Full_Legal_Name__c" variant="label-hidden" value="{!v.sapRecord.Full_Legal_Name__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><aura:if isTrue="{!v.sapRecord.On_boarding_Request_Type__c == 'New Sold-To w/ Ship-to &amp; Product'}"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr></aura:if> {!v.languageLabelMap.CA_3}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="DUNS_No__c" variant="label-hidden" value="{!v.sapRecord.DUNS_No__c}" required="{!v.sapRecord.On_boarding_Request_Type__c == 'New Sold-To w/ Ship-to &amp; Product'}"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><aura:if isTrue="{!v.sapRecord.On_boarding_Request_Type__c == 'New Sold-To w/ Ship-to &amp; Product'}"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr></aura:if> {!v.languageLabelMap.CA_4}</label>
                    <lightning:select variant="label-hidden" value="{!v.sapRecord.Is_Customer_a_Publically_Traded_Company__c}" disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" required="{!v.sapRecord.On_boarding_Request_Type__c == 'New Sold-To w/ Ship-to &amp; Product'}">
                        <option value="">--{!v.languageLabelMap.HP_45}--</option>
                        <option value="Yes">{!v.languageLabelMap.HP_31}</option>
                        <option value="No">{!v.languageLabelMap.HP_32}</option>
                    </lightning:select>
                    <!--lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Is_Customer_a_Publically_Traded_Company__c" variant="label-hidden" required="true" value="{!v.sapRecord.Is_Customer_a_Publically_Traded_Company__c}" /-->
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><aura:if isTrue="{!v.sapRecord.Is_Customer_a_Publically_Traded_Company__c == 'Yes'}"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr></aura:if> {!v.languageLabelMap.CA_5}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Stock_Ticker_Symbol__c" variant="label-hidden" value="{!v.sapRecord.Stock_Ticker_Symbol__c}" required="{!v.sapRecord.Is_Customer_a_Publically_Traded_Company__c == 'Yes'}"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_6}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Sold_To_Company_Name__c" variant="label-hidden" value="{!v.sapRecord.Sold_To_Company_Name__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_7}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Sold_To_Street__c" variant="label-hidden" value="{!v.sapRecord.Sold_To_Street__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"> {!v.languageLabelMap.CA_8}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Sold_To_Street_2__c" variant="label-hidden" value="{!v.sapRecord.Sold_To_Street_2__c}"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_9}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Sold_To_City__c" variant="label-hidden" value="{!v.sapRecord.Sold_To_City__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_10}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Sold_To_State_Province__c" variant="label-hidden" value="{!v.sapRecord.Sold_To_State_Province__c}" required="{!or(v.sapRecord.Sold_To_Country__c == 'United States', v.sapRecord.Sold_To_Country__c == 'Canada')}"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_11}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Sold_To_Postal_Code__c" variant="label-hidden" value="{!v.sapRecord.Sold_To_Postal_Code__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_12}</label>
                    <lightning:inputField disabled="{!or(and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress'), v.sapRecord.On_boarding_Request_Type__c != 'New Sold-To w/ Ship-to &amp; Product')}" fieldName="Sold_To_Country__c" variant="label-hidden" value="{!v.sapRecord.Sold_To_Country__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4"></div>
                <div class="slds-col slds-size_1-of-1" style="min-width:100%;border: 1px solid #d8d8d8;margin: 5px 0;"></div>
                <div class="slds-col slds-large-size_4-of-4 slds-medium-size_4-of-4 slds-small-size_4-of-4">
                    <div class="slds-form-element">
                        <label class="slds-form-element__label" for="text-input-id-1">&nbsp;</label>
                        <div class="slds-form-element__control">
                            <button class="slds-button slds-button_brand copyBtn" id="billToSameSoldTo" onclick="{!c.handleCopyAddresses}" type="button" disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}">
                                <lightning:icon iconName="utility:copy" class="copyBtnIcon" size="x-small" style="margin-right:5px;"/><b>{!v.languageLabelMap.CA_13}</b>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_14}</label>
                    <lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Bill_To_Company_Name__c" variant="label-hidden" value="{!v.sapRecord.Bill_To_Company_Name__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_15}</label>
                    <lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Bill_To_Street__c" variant="label-hidden" value="{!v.sapRecord.Bill_To_Street__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"> {!v.languageLabelMap.CA_16}</label>
                    <lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Bill_To_Street_2__c" variant="label-hidden" value="{!v.sapRecord.Bill_To_Street_2__c}" />
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_17}</label>
                    <lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Bill_To_City__c" variant="label-hidden" value="{!v.sapRecord.Bill_To_City__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_18}</label>
                    <lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Bill_To_State_Province__c" variant="label-hidden" value="{!v.sapRecord.Bill_To_State_Province__c}" required="{!or(v.sapRecord.Bill_To_Country__c == 'United States', v.sapRecord.Bill_To_Country__c == 'Canada')}"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_19}</label>
                    <lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Bill_To_Postal_Code__c" variant="label-hidden" value="{!v.sapRecord.Bill_To_Postal_Code__c}" required="true"/>
                </div>
                <div class="slds-col slds-large-size_1-of-4 slds-medium-size_2-of-4 slds-small-size_4-of-4">
                    <label  class="slds-form-element__label slds-no-flex"><abbr lightning-input_input="" title="required" class="slds-required">*</abbr> {!v.languageLabelMap.CA_20}</label>
                    <lightning:inputField disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}" fieldName="Bill_To_Country__c" variant="label-hidden" value="{!v.sapRecord.Bill_To_Country__c}" required="true"/>
                </div>
                <aura:if isTrue="{!v.sapRecord.On_boarding_Request_Type__c != 'New Ship-To &amp; Product'}"> 
                    <aura:if isTrue="{!and(v.vatList.length == 0,v.sapRecord.On_boarding_Request_Type__c != 'New Product')}">
                        <div class="slds-col slds-size_1-of-1 slds-m-top_medium" style="border-bottom:2px solid #767676;color:#767676;font-size: 14px;font-weight:bold;">{!v.languageLabelMap.CA_21}
                            <lightning:button label="{!v.languageLabelMap.CA_22}" variant="brand" class="addTaxButton slds-float_right slds-m-bottom_xx-small" onclick="{!c.addNewTax}" type="button"/>
                        </div>
                    </aura:if>
                    <aura:if isTrue="{!v.sapRecord.On_boarding_Request_Type__c != 'New Product'}">
                        <aura:iteration items="{!v.vatList}" var="vat" indexVar="vtIndx">
                            <div class="slds-col slds-size_1-of-1 slds-m-top_medium" style="border-bottom:2px solid #767676;color:#767676;font-size: 14px;font-weight:bold;">{!v.languageLabelMap.CA_21} {!vtIndx+1}
                                <aura:if isTrue="{!vtIndx+1 ==  v.vatList.length}">
                                    <lightning:button label="{!v.languageLabelMap.CA_22}" variant="brand" class="addTaxButton slds-float_right slds-m-bottom_xx-small" onclick="{!c.addNewTax}" type="button" disabled="{!and(v.sapRecord.Credit_Application_Status__c != 'Sent To Customer', v.sapRecord.Credit_Application_Status__c != 'Customer In Progress')}"/>
                                </aura:if>
                            </div>
                            <lightning:recordEditForm objectApiName="VAT__c" aura:id="vatForm" recordId="{!vat.Id}" onsuccess="{!c.handleVatSuccess}" 
                                                      onerror="{!c.handleError}">
