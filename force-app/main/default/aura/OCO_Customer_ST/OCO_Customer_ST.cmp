                    <button class="slds-button slds-button_neutral btn" type="submit" disabled="{!or(and(v.sapRecord.Product_Stewardship_Status__c != 'Sent To Customer', v.sapRecord.Product_Stewardship_Status__c != 'Customer In Progress'),v.isDisabledForProfile)}">
