<apex:page showHeader="false" sidebar="false" applyBodyTag="false" applyHtmlTag="false" standardController="SAP_Onboarding__c" renderAs="pdf" extensions="CustomerSetupPDFController">
    <html>
        <head>
            <style>
                @page {
                margin : 2em;
                }
                html *
                {
                color: #000 !important;
                font-family: Verdana, Arial, sans-serif !important;
                }
                .detailHeader{
                font-size: 18px;
                background:#d8d8d8;
                padding:5px;
                border: 1px solid #d8d8d8;
                }
                .fieldHeader{
                font-size: 12px;
                }
                .fieldValue{
                font-size:15px;
                font-weight:bold;
                }
                #content-details{
                padding-left : 10px;
                padding-right : 10px;
                margin-top:10px;
                }
                #content-details-child{
                padding-left : 10px;
                padding-right : 10px;
                margin-top:50px;
                }
                
                table, th, td {
                border: 1px solid #d8d8d8;
                border-collapse: collapse;
                
                }
                
                th{
                font-weight:bold;background-color:#ececec;
                width : 40% !important;
                }
                .marginTop{
                margin-top : 2em;
                }
                
            </style>
        </head>
        <body>
            <div id="container">
                <div id="header">
                    <div style="float:left">
                        <img src="{!$Resource.CreditApplicationLogo}" style="width:150px;"/>
