                        <th>Describe Special Billing Procedures Req.</th>
                        <td>{!SAP_Onboarding__c.Non_Standard_Billing_Procedure_Details__c}</td>
                    </tr>
                    
                    <tr>
                        <th>Additional Information</th>
                        <td>{!SAP_Onboarding__c.Additional_Information__c}</td>
                    </tr>
                </table>
               <apex:variable value="{!0}" var="rowNum"/>
                <apex:repeat value="{!vatList}" var="vat">
                    <apex:variable var="rowNum" value="{!rowNum + 1}"/>
                    <div class="detailHeader marginTop">
                        Tax/VAT {!rowNum}
